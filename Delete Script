HOTKEY = 'h' -- letter keys only
-- 23/12/2018 --
-- By Valualty --
-- The part you click will turn transparent and go through --
-- USER INPUT SERVICE DOESN'T WORK FOR EXPLOITS! --
wait(0.1)
plr = game:GetService('Players').LocalPlayer
mouse = plr:GetMouse()
tagn = 'gnomed'
actn = 'acto3'

function checkact()
	if workspace:FindFirstChild(actn) then
		return true
	end
end
function checkdeath()
	if plr.Character:FindFirstChild('Humanoid') then
		if plr.Character.Humanoid.Health <= 0 then
			return true
		end
	end
end

local h = Instance.new('Hint',plr.PlayerGui)
h.Text = "EXPLOIT BY VALUALTY"
game.Debris:AddItem(h,4)





spawn(function() -- just an introduction stuff

	local gui = Instance.new('ScreenGui',plr.PlayerGui)
	gui.Name = 'btool gui by valualty'
	local top = Instance.new("TextLabel")
	local under = Instance.new("TextLabel")
	local shadow = Instance.new("TextLabel")
	local howto = Instance.new("TextLabel")
	
	top.Name = "top"
	top.Parent = gui
	top.BackgroundColor3 = Color3.new(1, 1, 1)
	top.BackgroundTransparency = 1
	top.Position = UDim2.new(0.200000003, 0, 0.349999994, 0)
	top.Size = UDim2.new(0.600000024, 0, 0.300000012, 0)
	top.ZIndex = 2
	top.Font = Enum.Font.GothamBold
	top.Text = "EXPLOIT BY VALUALTY\nTwitter: @valualty"
	top.TextColor3 = Color3.new(1, 0.666667, 0)
	top.TextScaled = true
	top.TextSize = 14
	top.TextStrokeTransparency = 0
	top.TextWrapped = true
	
	under.Name = "under"
	under.Parent = gui
	under.BackgroundColor3 = Color3.new(1, 1, 1)
	under.BackgroundTransparency = 1
	under.Position = UDim2.new(0.200000003, 0, 0.349999994, 4)
	under.Size = UDim2.new(0.600000024, 0, 0.300000012, 0)
	under.Font = Enum.Font.GothamBold
	under.Text = "EXPLOIT BY VALUALTY\nTwitter: @valualty"
	under.TextColor3 = Color3.new(0.741176, 0.494118, 0)
	under.TextScaled = true
	under.TextSize = 14
	under.TextStrokeTransparency = 0
	under.TextWrapped = true
	
	shadow.Name = "shadow"
	shadow.Parent = gui
	shadow.BackgroundColor3 = Color3.new(1, 1, 1)
	shadow.BackgroundTransparency = 1
	shadow.Position = UDim2.new(0.192885369, 0, 0.349999994, 4)
	shadow.Size = UDim2.new(0.614229262, 0, 0.317350179, 0)
	shadow.ZIndex = 0
	shadow.Font = Enum.Font.GothamBold
	shadow.Text = "EXPLOIT BY VALUALTY\nTwitter: @valualty"
	shadow.TextColor3 = Color3.new(0, 0, 0)
	shadow.TextScaled = true
	shadow.TextSize = 14
	shadow.TextTransparency = 0.5
	shadow.TextWrapped = true
	
	howto.Name = "howto"
	howto.Parent = gui
	howto.BackgroundColor3 = Color3.new(1, 1, 1)
	howto.BackgroundTransparency = 1
	howto.Position = UDim2.new(0.349999994, 0, 0.939999998, -60)
	howto.Size = UDim2.new(0.300000012, 0, 0.0599999987, 0)
	howto.Font = Enum.Font.Highway
	howto.Text = "Press "..HOTKEY.." key to toggle Delete Tool"
	howto.TextColor3 = Color3.new(1, 0.666667, 0)
	howto.TextScaled = true
	howto.TextSize = 14
	howto.TextStrokeTransparency = 0
	howto.TextWrapped = true
	howto.Visible = false
	
	spawn(function() -- some dank anthem for panic station
		ussr = Instance.new('Sound',plr.PlayerGui)
		ussr.Name = 'BLYATSOME'
		ussr.SoundId = 'rbxassetid://593681991'
		ussr.TimePosition = 3.5
		ussr.Volume = 1
		ussr:Play()
		wait(10)
		for i = 1,-0.05,-0.05 do
			ussr.Volume = i
			wait()
		end
	end)
	
	wait(6)
	for i = 0,1.1,0.1 do
		top.TextStrokeTransparency = i
		top.TextTransparency = i
		under.TextStrokeTransparency = i
		under.TextTransparency = i
		shadow.TextStrokeTransparency = i
		shadow.TextTransparency = i
		wait()
	end
	
	howto.Visible = true
end)






keyevent = mouse.KeyDown:Connect(function(key)
	if not checkdeath() then
		if key == string.lower(HOTKEY) then
			if not workspace:FindFirstChild(actn) then
				local acttag = Instance.new('BoolValue',workspace)
				acttag.Name = actn
			else
				workspace[actn]:Destroy()
			end
		end
	end
end)

clickevent = mouse.Button1Down:Connect(function()
	if checkact() then
		local target = mouse.Target
		if target then
			if not target:FindFirstChild(tagn) then
				local tag = Instance.new('BoolValue',target)
				tag.Name = tagn
				local trans = Instance.new('NumberValue',target)
				trans.Name = 'trans'
				trans.Value = target.Transparency
				local clickbox = Instance.new('SelectionBox',target)
				clickbox.Name = 'cliccbox'
				clickbox.LineThickness = .05
				clickbox.Adornee = target
				clickbox.Color3 = Color3.fromRGB(255,0,0)
				
				if target.Transparency < 0.7 then
					target.Transparency = 0.7
				end
				target.CanCollide = false
			else
				target.Transparency = target.trans.Value
				target.cliccbox:Destroy()
				target.CanCollide = true
				
				target[tagn]:Destroy()
				target.trans:Destroy()
			end
		end
	end
end)

while true do
	wait(.1)
	if checkact() then
		local target = mouse.Target
		if target then
			local hoverbox = Instance.new('SelectionBox',target)
			hoverbox.Name = 'hoverbox'
			hoverbox.LineThickness = .1
			hoverbox.Color3 = Color3.fromRGB(85, 255, 255)
			hoverbox.Adornee = target
			game.Debris:AddItem(hoverbox,.1)
		end
	end
	if checkdeath() then
		break
	end
end

if checkact() then workspace[actn]:Destroy() end
if keyevent then keyevent:Disconnect() end
if clickevent then clickevent:Disconnect() end
